<%- include('../partials/adminheader.ejs') -%>
<%- include('../partials/adminnavbar.ejs') -%>


<h2 class="page-title">Edit a category</h2>
<a href="/admin/categories" class= "btn btn-primary">Back to all categories</a>
<br><br>
<form action="/admin/categories/edit-category/<%= id %>" method = "post">
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" name = "title" class = "form-control" value="<%= title %>" placeholder="Title">
    </div>

    <button class="btn btn-primary">Submit</button>

</form>


